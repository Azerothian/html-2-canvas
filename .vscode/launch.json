{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Node Debug",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/build/server/index.js",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}",
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "*"
      },
      "console": "integratedTerminal",
      "sourceMaps": true,
      "outDir": "${workspaceRoot}/build/"
    }
  ]
}